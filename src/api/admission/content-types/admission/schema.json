{
  "kind": "collectionType",
  "collectionName": "admissions",
  "info": {
    "singularName": "admission",
    "pluralName": "admissions",
    "displayName": "Admission"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name_title": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Mr.",
        "Ms."
      ]
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "date_of_birth": {
      "type": "date",
      "required": true
    },
    "nationality": {
      "type": "string",
      "required": true
    },
    "passport_size_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "mobile_no": {
      "type": "biginteger"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "Language_Proficiency": {
      "type": "component",
      "component": "language-and-proficiency.language-proficiency",
      "repeatable": false
    },
    "address": {
      "type": "blocks"
    },
    "city": {
      "type": "string"
    },
    "district": {
      "type": "string"
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "admissions"
    },
    "pincode": {
      "type": "string"
    },
    "hobbies": {
      "type": "string"
    },
    "photography_club": {
      "type": "string"
    },
    "blood_group": {
      "type": "string"
    },
    "Parent_Guardian_Spouse_Details": {
      "type": "component",
      "component": "parent-guardian-spouse.parent-guardian-spouse-details",
      "repeatable": false
    },
    "step_1": {
      "type": "boolean",
      "default": false
    },
    "Education_Details": {
      "type": "component",
      "component": "education-details.education-details",
      "repeatable": false
    },
    "Under_Graduate": {
      "type": "component",
      "component": "under-graduate.under-graduate",
      "repeatable": false
    },
    "Post_Graduate": {
      "type": "component",
      "component": "post-graduate.post-graduate",
      "repeatable": true
    },
    "Work_Experience": {
      "type": "component",
      "component": "work-experience.work-experience",
      "repeatable": true
    },
    "step_2": {
      "type": "boolean",
      "default": false
    },
    "Upload_Your_Portfolio": {
      "type": "component",
      "component": "upload-your-portfolio.upload-your-portfolio",
      "repeatable": false
    },
    "step_3": {
      "type": "boolean",
      "default": false
    },
    "Payment_Status": {
      "type": "enumeration",
      "enum": [
        "Paid",
        "UnPaid"
      ]
    }
  }
}
